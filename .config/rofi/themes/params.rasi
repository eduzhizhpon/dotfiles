/* Color-Scheme For All Themes*/
@import "theme.rasi"

* {
  background: #1d1d1dff;
  background-alt: #2e2e2eff;
  foreground: #eeeeee;
  placeholder: #888888ff;
  border: @theme-color;
  selected: @theme-color;
  urgent: #ec407aff;
  logo: #ec407aff;
  on: #61c766ff;
  off: #ec7875ff;

  window-border-color: #555555;
  window-border-radius: 16px;
  input-border-radius: 8px;
}
